root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true

[*.yml]
indent_style = space

[*.meta]
indent_style = space
indent_size = 2
trim_trailing_whitespace = false

[*.asmdef]
indent_style = space
indent_size = 4
trim_trailing_whitespace = false
